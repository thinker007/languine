{
    "$schema": "https://turborepo.org/schema.json",
    "globalDependencies": ["**/.env"],
    "ui": "stream",
    "tasks": {
      "topo": {
        "dependsOn": ["^topo"]
      },
      "build": {
        "inputs": ["$TURBO_DEFAULT$", ".env"],
        "dependsOn": ["^build"],
        "outputs": [".next/**", "!.next/cache/**", "next-env.d.ts", ".expo/**"]
      },
      "start": {
        "cache": false
      },
      "test": {
        "cache": false
      },
      "dev": {
        "inputs": ["$TURBO_DEFAULT$", ".env"],
        "persistent": true,
        "cache": false
      },
      "format": {},
      "lint": {
        "dependsOn": ["^topo"]
      },
      "typecheck": {
        "dependsOn": ["^topo"],
        "outputs": ["node_modules/.cache/tsbuildinfo.json"]
      }
    }
  }
  